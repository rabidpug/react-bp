{
  "name": "react-bp",
  "version": "0.1.0",
  "description": "A react boilerplate",
  "main": "index.js",
  "scripts": {
    "start:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.babel.js --mode development",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "test": "eslint src && flow && jest --coverage",
    "precommit": "npm run test",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://rabidpug@github.com/rabidpug/react-bp.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Matt Cuneo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rabidpug/react-bp/issues"
  },
  "homepage": "https://github.com/rabidpug/react-bp#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "^6.0.1",
    "flow-bin": "^0.66.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.7",
    "start-server-webpack-plugin": "^2.2.3",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.6.0"
  },
  "browserslist": [
    "> 1%"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "mongoose": "^5.0.8",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3"
  }
}
