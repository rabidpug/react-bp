  },
  "scripts": {
    "build": "npm-run-all clean test build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.client.js --mode production --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.server.js --mode production",
    "dev": "npm-run-all clean dev:*",
    "start": "node dist/server",
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.server.js --mode development",
    "prod": "heroku local",
    "clean": "rimraf ./dist",
    "test": "eslint src && flow && jest --coverage",
    "dist": "cross-env NODE_ENV=production node dist/server.js",
    "heroku-postbuild": "npm-run-all clean build:*"
  },
  "engines": {
    "node": "9.x",
    "yarn": "1.x"
  },
  "version": "0.1.0"
