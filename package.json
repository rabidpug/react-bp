  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:aserver":
      "cross-env NODE_ENV=production webpack --config webpack/webpack.config.server.js --mode production",
    "build:client":
      "cross-env NODE_ENV=production webpack --config webpack/webpack.config.client.js --mode production --progress",
    "dev": "npm-run-all clean dev:* --parallel",
    "dev:server":
      "cross-env NODE_ENV=development  webpack --config webpack/webpack.config.server.js --mode development",
    "dev:cssflow": "css-modules-flow-types src -e scss --watch",
    "test": "npm-run-all test:*",
    "test:cssflow": "cross-env NODE_ENV=test css-modules-flow-types src -e scss",
    "test:lint": "cross-env NODE_ENV=test eslint src",
    "test:flow": "cross-env NODE_ENV=test flow",
    "test:jest": "cross-env NODE_ENV=test jest --coverage",
    "start": "node dist/server",
    "prod": "heroku local",
    "clean": "rimraf ./dist",
    "dist": "cross-env NODE_ENV=production node dist/server.js",
    "keys": "web-push generate-vapid-keys",
    "heroku-postbuild": "npm-run-all clean build:*",
    "snapshots": "jest --updateSnapshot"
  },
  "engines": {
    "node": "9.x",
    "yarn": "1.x"
  },
  "version": "0.1.0"
